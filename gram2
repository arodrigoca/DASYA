<PROG> ::= <FUNC> <PROG> |
           <FUNC> <EOF> |
           <EOF>

<FUNC> ::= <FSIG> '{' <BODY> '}'

<FSIG> ::= 'func' <ID> '(' <FARGS> ')' |
           'func' <ID> '(' ')'

<FARGS> ::= 'int' <ID>  ',' <FARGS> |
            'int' <ID> |

<BODY> ::= <STMNT> <BODY> |
           <STMNT>

<STMNT> :: = <FUNC>
             <DEC>
             <ITER>

<DEC> ::= <ID> '(' <ATOM>, <ATOM>, <ATOM>, <ATOM> ')' ";" |
          'int' <ID> '=' <ATOM> ';'

<ATOM> ::= <ID> |
           intval

<ITER> ::= 'iter' '(' <ID> ':=' <ATOM> ';' <ATOM> ',' <ATOM> ')' '{' <BODY> '}'
