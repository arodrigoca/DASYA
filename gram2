<PROG> ::= <FUNC> <PROG> |
           <FUNC> <EOF> |
           <EOF>

<FUNC> ::= <FSIG> '{' <BODY> '}'

<FSIG> ::= 'func' <ID> '(' <FDECARGS> ')' |
           'func' <ID> '(' ')'

<FDECARGS> ::= <DTYPE> <ID>  ',' <FARGS> |
               <DTYPE> <ID> |

<BODY> ::= <STMNT> <BODY> |
           <STMNT>

<STMNT> ::= <FCALL>
            <DECL>
            <ITER>

<FCALL> ::= <ID> '(' <FARGS> ')' ";"

<FARGS> ::= <EXPR> ',' <FARGS> |
            <EXPR>

<DECL> ::= <DTYPE> <ID> '=' <EXPR> ';'

<EXPR> :: = <ATOM>

<ATOM> ::= <ID> |
           intval

<ITER> ::= 'iter' '(' <ID> ':=' <EXPR> ';' <EXPR> ',' <EXPR> ')' '{' <BODY> '}'
